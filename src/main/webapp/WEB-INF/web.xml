<?xml version="1.0" encoding="UTF-8"?>

<web-app>


	<security-constraint>
		<web-resource-collection>
			<web-resource-name>Automatic SSL Forward</web-resource-name>
			<url-pattern>/*</url-pattern>
			<http-method>GET</http-method>
			<http-method>POST</http-method>
		</web-resource-collection>
	</security-constraint>

 </web-app>